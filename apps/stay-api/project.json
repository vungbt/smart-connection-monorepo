{
  "name": "stay-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/stay-api",
  "projectType": "application",
  "targets": {
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["rm -rf dist/apps/stay-api", "rm -rf apps/stay-api/dist"]
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/stay-api",
        "main": "apps/stay-api/src/main.ts",
        "tsConfig": "apps/stay-api/tsconfig.json",
        "assets": ["apps/stay-api/src/assets"],
        "buildableProjectDepsInPackageJsonType": "dependencies"
      },
      "configurations": {
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        },
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "sourceMap": false
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "stay-api:build",
        "watch": true
      },
      "configurations": {
        "development": {
          "buildTarget": "stay-api:build:development"
        },
        "production": {
          "buildTarget": "stay-api:build:production"
        }
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
        "cwd": "apps/stay-api"
      }
    }
  },
  "tags": []
}
